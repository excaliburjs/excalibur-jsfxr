  "license": "BSD-2-Clause",
  "keywords": [
    "excalibur",
    "game",
    "engine",
    "sfx",
    "jsfxr",
    "sound",
    "audio"
  ],
  "bugs": {
    "url": "https://github.com/excaliburjs/excalibur-jsfxr/issues"
  },
  "homepage": "https://github.com/excaliburjs/excalibur-jsfxr#readme",
  "devDependencies": {
    "copyfiles": "2.4.1",
    "excalibur": "^0.30.1",
