{
  "name": "@excaliburjs/plugin-jsfxr",
  "version": "0.30.1",
  "description": "excalibur-jsfxr provides sound effect generation utilizing a wrapper around jsfxr",
  "main": "dist/excalibur-jsfxr.min.js",
  "types": "dist/excalibur-jsfxr.d.ts",
  "scripts": {
    "build": "tsc && npm run minify && copyfiles --flat src/riffwave.mjs src/sfxr.mjs dist",
